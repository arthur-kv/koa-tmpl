{
  "name": "koa-tmpl",
  "version": "1.0.0",
  "description": "A template for Koa.js applications",
  "main": "index.js",
  "scripts": {
    "lint": "node ./node_modules/.bin/eslint . --color --format stylish --fix --ignore-path .gitignore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "node ./node_modules/.bin/eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arthur-kv/koa-tmpl.git"
  },
  "author": "Arthur Kashaiev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arthur-kv/koa-tmpl/issues"
  },
  "homepage": "https://github.com/arthur-kv/koa-tmpl#readme",
  "dependencies": {},
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0"
  }
}
